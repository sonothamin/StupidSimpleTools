{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/tools/(.*)",
            "dest": "/tools/$1",
            "headers": {
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}

{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "tools/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/tools/(.*)",
        "dest": "/tools/$1.js"
      },
      {
        "src": "/",
        "dest": "/index.js"
      }
    ]
  }
  